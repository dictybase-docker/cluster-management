{
  "name": "cluster-automation-k0s",
  "version": "1.0.0",
  "main": "main.js",
  "types": "main.ts",
  "license": "MPL-2.0",
  "private": true,
  "scripts": {
    "get": "cdktf get",
    "build": "tsc",
    "synth": "cdktf synth",
    "compile": "tsc --pretty",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "upgrade": "yarn upgrade cdktf@latest cdktf-cli@latest",
    "upgrade:next": "yarn upgrade cdktf@next cdktf-cli@next"
  },
  "engines": {
    "node": ">=14.0"
  },
  "dependencies": {
    "@cdktf/provider-google": "5.0.8",
    "cdktf": "^0.15.5",
    "constructs": "^10.1.276"
  },
  "devDependencies": {
    "@swc/core": "^1.3.34",
    "@swc/jest": "^0.2.24",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.15.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.4",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest"
      ]
    },
    "setupFiles": [
      "<rootDir>/test-environment.ts"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/setup-jest-cdktf.js"
    ]
  }
}
