{
    "name": "cluster-management",
    "version": "1.0.0",
    "main": "src/main.ts",
    "types": "src/main.ts",
    "private": true,
    "type": "module",
    "scripts": {
        "get": "cdktf get",
        "build": "tsc",
        "synth": "cdktf synth",
        "deploy": "cdktf deploy --auto-approve",
        "destroy": "cdktf destroy",
        "compile": "tsc --pretty",
        "watch": "tsc -w",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "upgrade": "yarn upgrade cdktf@latest cdktf-cli@latest",
        "upgrade:next": "yarn upgrade cdktf@next cdktf-cli@next",
        "cluster:config": "yarn tsx src/generate_cluster_config.ts"
    },
    "engines": {
        "node": ">=18.0"
    },
    "dependencies": {
        "@cdktf/provider-google": "7.x",
        "@cdktf/provider-helm": "^6.0.0",
        "@cdktf/provider-kubernetes": "^6.0.1",
        "@cdktf/provider-local": "^6.0.1",
        "@kubernetes/client-node": "0.18",
        "@octokit/rest": "^19.0.11",
        "cdktf": "0.16.x",
        "cdktf-cli": "0.16.x",
        "eta": "^2.2.0",
        "ssh2": "^1.13.0",
        "winston": "^3.9.0",
        "yaml": "^2.3.1",
        "yargs": "^17.7.1"
    },
    "devDependencies": {
        "@swc/core": "^1.3.62",
        "@swc/jest": "^0.2.24",
        "@types/jest": "^29.5.2",
        "@types/node": "^18.16.0",
        "@types/ssh2": "^1.11.11",
        "jest": "^29.5.0",
        "msw": "^1.2.1",
        "prettier": "^2.8.8",
        "tsx": "^3.12.7",
        "typescript": "^5.1.3"
    },
    "jest": {
        "transform": {
            "^.+\\.(t|j)sx?$": [
                "@swc/jest"
            ]
        },
        "setupFilesAfterEnv": [
            "<rootDir>/__tests__/setup.ts"
        ],
        "testRegex": "/__tests__/.*_test\\.ts$"
    }
}
